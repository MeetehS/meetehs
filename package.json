  "license": "ISC",
  "scripts": {
    "precommit": "lint-staged",
    "flow": "flow",
    "analyze": "source-map-explorer build/static/js/main.*",
    "start:client": "react-scripts start",
    "start:server": "nodemon --watch server server",
