    "eject": "react-scripts eject",
    "deploy": "pm2 deploy ecosystem.config.js production"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.2",
    "apollo-client": "^2.1.0",
    "apollo-link-http": "^1.3.0",
    "bootstrap": "^3.3.7",
    "graphql": "^0.11.7",
    "graphql-server-koa": "^1.2.0",
    "graphql-tag": "^2.5.0",
    "graphql-tools": "^2.11.0",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.1.0",
    "koa-compress": "^2.0.0",
    "koa-router": "^7.3.0",
    "koa-send": "^4.1.1",
    "koa-static": "^4.0.2",
    "marked": "^0.3.7",
    "moment": "^2.19.3",
    "mongoose": "^4.13.6",
    "react": "^16.2.0",
    "react-apollo": "^2.0.1",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "flow-bin": "^0.60.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.8.2",
    "react-scripts": "^1.0.17",
    "source-map-explorer": "^1.5.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --singl3e-quote --write",
      "git add"
    ]
  },
  "proxy": "http://localhost:4000"
}
